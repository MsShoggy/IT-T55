#include<stdio.h>
void main()
{
     char name[50];
     int age;
     
     printf("Enter name of person\t");
     scanf("%s",&name);
     printf("Enter Age of Person\t");
     scanf("%d",&age);
     
     if(age>17)
     
          printf("\nPerson is Eligible for Voting");
     else
          printf("\nPerson is NOT Eligible for Voting");
    
     return 0;
}